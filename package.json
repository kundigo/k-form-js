{
  "name": "k-form-js",
  "version": "0.1.0",
  "description": "Enhanced form components, inspired by formik, designed to work with server generated forms by Ruby on Rails",
  "author": {
    "name": "Dorian LUPU",
    "email": "dorian@kundigo.pro",
    "url": "https://twitter.com/dorianlupu"
  },
  "contributors": [
  ],
  "license": "Private code that belongs to KUNDIGO SASU, FRANCE",
  "main": "dist/k-form-js.ssr.js",
  "module": "dist/k-form-js.esm.js",
  "unpkg": "dist/k-form-js.min.js",
  "files": [
    "dist/*",
    "src/**/*.vue",
    "!src/lib-dev.vue"
  ],
  "scripts": {
    "build": "cross-env NODE_ENV=production rollup --config build/rollup.config.js",
    "build:ssr": "cross-env NODE_ENV=production rollup --config build/rollup.config.js --format cjs",
    "build:es": "cross-env NODE_ENV=production rollup --config build/rollup.config.js --format es",
    "build:unpkg": "cross-env NODE_ENV=production rollup --config build/rollup.config.js --format iife"
  },
  "dependencies": {
    "moment": "^2.24.0"
  },
  "devDependencies": {
    "cross-env": "^6.0.3",
    "minimist": "^1.2.0",
    "rollup": "^1.26.1",
    "rollup-plugin-buble": "^0.19.8",
    "rollup-plugin-commonjs": "^10.1.0",
    "rollup-plugin-replace": "^2.2.0",
    "rollup-plugin-terser": "^5.1.2",
    "rollup-plugin-vue": "5.1.1",
    "vue": "^2.6.10",
    "vuex": "^3.1.1",
    "vue-template-compiler": "^2.6.10"
  }
}
